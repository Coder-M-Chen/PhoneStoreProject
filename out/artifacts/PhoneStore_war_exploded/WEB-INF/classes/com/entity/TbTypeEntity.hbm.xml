<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.TbTypeEntity" table="tb_type" schema="phonestore">
        <id name="typeId">
            <column name="TypeID" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="typeName">
            <column name="TypeName" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>